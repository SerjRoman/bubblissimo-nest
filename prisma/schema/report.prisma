model Report {
  id          String    @id @default(auto()) @map("_id") @db.ObjectId
  classroomId String    @db.ObjectId
  classroom   Classroom @relation(fields: [classroomId], references: [id], name: "ReportClassroomRelation")
  sessionId   String    @db.ObjectId
  session     Session   @relation(fields: [sessionId], references: [id], name: "SessionReportRelation")
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
}
